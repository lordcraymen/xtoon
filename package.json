{
  "name": "xtoon",
  "version": "0.0.0-dev",
  "description": "A tool that **expands** XTOON authoring syntax into plain XML and **lints** authoring blocks for determinism",
  "keywords": [
    "xml",
    "compaction",
    "structured",
    "data",
    "efficient",
    "semantic",
    "markup"
  ],
  "homepage": "https://github.com/lordcraymen/xtoon#readme",
  "bugs": {
    "url": "https://github.com/lordcraymen/xtoon/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lordcraymen/xtoon.git"
  },
  "license": "MIT",
  "author": "Florian \"lordcraymen\" Patzke",
  "type": "module",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "vitest run",
    "commit": "cz",
    "lint": "echo 'Linter not configured yet'",
    "type-check": "echo 'Type check not configured yet'"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "semantic-release": "^22.0.12"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
